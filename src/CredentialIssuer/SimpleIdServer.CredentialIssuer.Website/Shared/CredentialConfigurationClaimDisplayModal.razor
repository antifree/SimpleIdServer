@inject DialogService dialogService

@using SimpleIdServer.CredentialIssuer.Domains;

<ClaimViewer Translation=@Translation>
    <ActionsContent>
        <RadzenButton Click=@((evt) => Save()) Variant="Variant.Text">@Global.Save</RadzenButton>
    </ActionsContent>
</ClaimViewer>

@code {
    [Parameter]
    public CredentialConfigurationTranslation Translation { get; set; }

    void Save()
    {
        dialogService.Close(Translation);
    }
}
